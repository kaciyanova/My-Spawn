{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "SuperSorter",
    "version": "0.4.4",	
    "description": "Clean up your bookmark mess! Sort bookmarks recursively, delete duplicates, merge folders, sort automatically, etc.",
    "manifest_version": 2,
	"background": {
		"page": "background.html"
	},
    "browser_action": {
        "default_icon": "go-jump-48.png",
        "default_title": "Sort bookmarks"		
    },
	"options_page": "options.html",
	"icons" : {
		"48" : "go-jump-48.png",
		"128" : "go-jump-128.png"
	},
    "permissions": [
		"bookmarks"
    ]
}