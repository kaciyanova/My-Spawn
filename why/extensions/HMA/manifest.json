{
"update_url": "https://clients2.google.com/service/update2/crx",

   "name": "Hide My Ass! Web Proxy",
   "version": "1.3.2",
   "manifest_version": 2,
   "description": "Official HMA! Web Proxy extension; easily switch to secure anonymous browsing.",
   "background": {
      "scripts": ["js/main.js"]
   },
   "browser_action": {
      "default_icon": {
         "19" : "images/icon.19.png",
         "38" : "images/icon.38.png"
      },
      "default_title": "Hide My Ass! Proxy"
   },   
   "icons": {
      "128": "images/icon.128.png"
   },
   "incognito": "spanning",
   "minimum_chrome_version": "6",
   "omnibox": {
      "keyword": "hma"
   },
   "options_page": "options.html",
   "permissions": [ "tabs", "contextMenus" ]   
}
